--PROGRAM--
// Some inspiration was taken from: https://github.com/python/cpython/blob/main/Lib/test/test_float.py

// TODO: we should have a native way to generate these values
inf := 10.0/0.0;
ninf := 10.0/-0.0;
nan := 0.0/0.0;

assert(4.20 == 4.20);

// TODO: this function has not yet been implemented
// print(float(420) == 420.0);

assert(1.0 + 2.0 == 3.0);
assert(3.0 - 2.0 == 1.0);
assert(5.0 * 3.0 == 15.0);
assert(10.0 / 2.0 == 5.0);

// Modulo stuff
assert(-1.0 % 1.0 == -0.0);
// TODO: add more at some point but getting good valid tests cases is extremely frustrating

// Total ordering
assert(nan == nan);
assert(ninf + inf == nan);
assert(nan > inf);
assert(inf > ninf);
assert(nan == -nan, "nan equals negative nan");

print("ok");
--EXPECT--
ok