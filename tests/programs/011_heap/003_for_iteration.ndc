fn test(heap) {
  heap.push(10);
  heap.push(9);
  heap.push(8);
  heap.push(11);
  heap.push(12);

  let out = [];
  for element in heap {
    out.push(element);
  }

  // We don't want this test to have a hard dependency on the internal representation of BinaryHeap
  assert_eq(out.sorted, [8,9,10,11,12]);
}

test(MinHeap());
test(MaxHeap());
