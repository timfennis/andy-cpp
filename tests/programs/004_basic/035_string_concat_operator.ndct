--PROGRAM--
print("foo" <> "bar");
print("foo" <> 123);
print(123 <> 123);
assert(123 <> 123 == "123123");
print("list: " <> [x for x in 1..=3]);
//TODO: maybe add a test case for map/set to string conversion once it's stable
print((1,2,3) <> 5);

string := "hello ";
string <>= "world!";
print(string);
--EXPECT--
foobar
foo123
123123
list: [1,2,3]
(1,2,3)5
hello world!
