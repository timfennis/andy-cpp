let l = [1,2,3];
assert_eq(l.map(fn(x) { x + 1 }), [2,3,4]);

let s = "foo";
assert_eq(s.map(fn(x) { "[" <> x <> "]" }), ["[f]", "[o]", "[o]"]);

let ss = %{"foo", "bar", "baz"};
assert_eq(
    ss.map(
        fn(tup) { let key, value = tup; key <> "f" }
    ).set(),
    %{"foof", "barf", "bazf"}
);

