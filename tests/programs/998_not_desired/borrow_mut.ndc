// expect-error: cannot mutate a value in a list while you're iterating over this list
fn mut_map(list, function) {

let   index = 0;
  for item in list {
let     new = function(item);
    list[index] = new; // Borrows list as mutable while it's still borrowed for item
    index += 1;
  }
}

let list = [1,2,3,4];
fn add_one(n) { n + 1 };

list.mut_map(add_one);
print(list);
